<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <!-- Main window -->
    <v:String x:Key="m_WeaponType">იარაღის ტიპი:</v:String>
    <v:String x:Key="m_Skin">Კანი:</v:String>
    <v:String x:Key="m_Quality">ხარისხი:</v:String>
    <v:String x:Key="m_Outcomes">შედეგები:</v:String>
    <v:String x:Key="m_SearchMode">ძიების რეჟიმი:</v:String>
    <v:String x:Key="m_DesiredFloat">სასურველი ფლოტი:</v:String>
    <v:String x:Key="m_Count">დათვლა:</v:String>
    <v:String x:Key="m_Skip">გამოტოვება:</v:String>
    <v:String x:Key="m_Start">დაწყება</v:String>
    <v:String x:Key="m_Stop">გაჩერდი</v:String>
    <v:String x:Key="m_FindOne">იპოვე ერთი</v:String>
    <v:String x:Key="m_Sort">დალაგება</v:String>
    <v:String x:Key="m_Descending">Დაღმავალი</v:String>
    <v:String x:Key="m_ThreadCount">თემების რაოდენობა:</v:String>
    <v:String x:Key="m_CraftRange">ხელნაკეთობების დიაპაზონი:</v:String>
    <v:String x:Key="m_CurrentSpeed">მიმდინარე სიჩქარე:</v:String>
    <v:String x:Key="m_CombinationsPerSecond">კომბინაციები/მეორე</v:String>
    <v:String x:Key="m_CheckedCombinations">შემოწმებული კომბინაციები:</v:String>
    <v:String x:Key="m_SkinNotFound">შეცდომა! ამ კანს არ შეიძლება ჰქონდეს ეს ხარისხი.</v:String>

    <!-- Settings window -->
    <v:String x:Key="m_Settings">პარამეტრები</v:String>
    <v:String x:Key="m_Language">Ენა:</v:String>
    <v:String x:Key="m_Currency">ვალუტა:</v:String>
    <v:String x:Key="m_Theme">თემა:</v:String>
    <v:String x:Key="m_GetThemes">მიიღეთ თემები</v:String>
    <v:String x:Key="m_OpenThemesFolder">გახსენით თემების საქაღალდე</v:String>
    <v:String x:Key="m_EnableSound">ხმის ჩართვა</v:String>
    <v:String x:Key="m_CheckForUpdates">Შეამოწმოთ განახლებები</v:String>
    
    <!-- Benchmark window -->
    <v:String x:Key="m_Benchmark">ნიშნული</v:String>
    <v:String x:Key="m_MultipleThreads">მრავალი ძაფი</v:String>
    <v:String x:Key="m_SingleThread">ერთი ძაფი</v:String>
    <v:String x:Key="m_StartBenchmark">დაწყება საორიენტაციო</v:String>
    <v:String x:Key="m_Update">განახლება</v:String>
    <v:String x:Key="m_Threads">ძაფები</v:String>
    <v:String x:Key="m_Thread">ძაფი</v:String>
    <v:String x:Key="m_PublishResult">შედეგის გამოქვეყნება</v:String>
    
    <!-- Combination -->
    <v:String x:Key="m_Copy">კოპირება</v:String>
    <v:String x:Key="m_PossibleFloat">შესაძლო ცურვა:</v:String>
    <v:String x:Key="m_Ingredients">ინგრედიენტები:</v:String>
    <v:String x:Key="m_Price">ფასი:</v:String>

    <!-- Discord RPC -->
    <v:String x:Key="m_SettingUpSearch">ძიების დაყენება</v:String>
    <v:String x:Key="m_Benchmarking">Benchmarking</v:String>
    <v:String x:Key="m_Searching">ეძებს</v:String>

    <!-- Status bar -->
    <v:String x:Key="m_SearchingSkin">კანის ძებნა ბაზარზე</v:String>
    <v:String x:Key="m_GettingFloats">ფლოტის მიღება ბაზრიდან</v:String>
    <v:String x:Key="m_ErrorCouldntGetFloats">შეცდომა! ბაზრიდან მოძრავი ფურცლების მიღება ვერ მოხერხდა</v:String>
    <v:String x:Key="m_ErrorLessThan10">შეცდომა! ბაზრიდან 10-ზე მეტი ცურვის მიღება ვერ მოხერხდა</v:String>
    <v:String x:Key="m_StartingUpSearch">ძიების დაწყება</v:String>
    <v:String x:Key="m_FinishedSearching">დაასრულა ძებნა</v:String>
    
    <!-- Update window -->
    <v:String x:Key="m_UpdateAvailable">Განახლება შესაძლებელია</v:String>
    <v:String x:Key="m_Later">მოგვიანებით</v:String>
    <v:String x:Key="m_DoNotAsk">Არ იკითხო</v:String>
</ResourceDictionary>