<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <!-- Main window -->
    <v:String x:Key="m_WeaponType">Type d'arme :</v:String>
    <v:String x:Key="m_Skin">La peau:</v:String>
    <v:String x:Key="m_Quality">Qualité:</v:String>
    <v:String x:Key="m_Outcomes">Résultats:</v:String>
    <v:String x:Key="m_SearchMode">Mode de recherche:</v:String>
    <v:String x:Key="m_DesiredFloat">Flotteur souhaité:</v:String>
    <v:String x:Key="m_Count">Compter:</v:String>
    <v:String x:Key="m_Skip">Sauter:</v:String>
    <v:String x:Key="m_Start">Démarrer</v:String>
    <v:String x:Key="m_Stop">Arrêt</v:String>
    <v:String x:Key="m_FindOne">Trouvez-en un</v:String>
    <v:String x:Key="m_Sort">Sorte</v:String>
    <v:String x:Key="m_Descending">Descendant</v:String>
    <v:String x:Key="m_ThreadCount">Nombre de fils:</v:String>
    <v:String x:Key="m_CraftRange">Gamme artisanale:</v:String>
    <v:String x:Key="m_CurrentSpeed">Vitesse actuelle:</v:String>
    <v:String x:Key="m_CombinationsPerSecond">combinaisons/seconde</v:String>
    <v:String x:Key="m_CheckedCombinations">Combinaisons cochées:</v:String>
    <v:String x:Key="m_SkinNotFound">Erreur! Cette peau ne peut pas avoir cette qualité.</v:String>

    <!-- Settings window -->
    <v:String x:Key="m_Settings">Réglages</v:String>
    <v:String x:Key="m_Language">Langue:</v:String>
    <v:String x:Key="m_Currency">Monnaie:</v:String>
    <v:String x:Key="m_Theme">Thème:</v:String>
    <v:String x:Key="m_GetThemes">Obtenir thèmes</v:String>
    <v:String x:Key="m_OpenThemesFolder">Ouvrir le dossier des thèmes</v:String>
    <v:String x:Key="m_EnableSound">Activer le son</v:String>
    <v:String x:Key="m_CheckForUpdates">Vérifier les mises à jour</v:String>
    
    <!-- Benchmark window -->
    <v:String x:Key="m_Benchmark">Référence</v:String>
    <v:String x:Key="m_MultipleThreads">Plusieurs fils</v:String>
    <v:String x:Key="m_SingleThread">Fil unique</v:String>
    <v:String x:Key="m_StartBenchmark">Commencer le benchmark</v:String>
    <v:String x:Key="m_Update">Mise à jour</v:String>
    <v:String x:Key="m_Threads">Fils</v:String>
    <v:String x:Key="m_Thread">Le fil</v:String>
    <v:String x:Key="m_PublishResult">Publier le résultat</v:String>
    
    <!-- Combination -->
    <v:String x:Key="m_Copy">Copie</v:String>
    <v:String x:Key="m_PossibleFloat">Flottant éventuel:</v:String>
    <v:String x:Key="m_Ingredients">Ingrédients:</v:String>
    <v:String x:Key="m_Price">Prix:</v:String>

    <!-- Discord RPC -->
    <v:String x:Key="m_SettingUpSearch">Configuration de la recherche</v:String>
    <v:String x:Key="m_Benchmarking">Analyse comparative</v:String>
    <v:String x:Key="m_Searching">Recherche</v:String>

    <!-- Status bar -->
    <v:String x:Key="m_SearchingSkin">Recherche de peau sur le marché</v:String>
    <v:String x:Key="m_GettingFloats">Obtenir des flotteurs du marché</v:String>
    <v:String x:Key="m_ErrorCouldntGetFloats">Erreur! Impossible d'obtenir des flotteurs du marché</v:String>
    <v:String x:Key="m_ErrorLessThan10">Erreur! Impossible d'obtenir plus de 10 flotteurs du marché</v:String>
    <v:String x:Key="m_StartingUpSearch">Lancement de la recherche</v:String>
    <v:String x:Key="m_FinishedSearching">Recherche terminée</v:String>
    
</ResourceDictionary>