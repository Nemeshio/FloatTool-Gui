    <v:String x:Key="m_FinishedSearching">Haku on valmis</v:String>
    
    <!-- Update window -->
    <v:String x:Key="m_UpdateAvailable">Päivitys saatavilla</v:String>
    <v:String x:Key="m_Later">Myöhemmin</v:String>
    <v:String x:Key="m_DoNotAsk">Älä kysy</v:String>
