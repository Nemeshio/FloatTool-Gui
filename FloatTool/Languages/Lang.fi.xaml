    <!-- Main window -->
    <v:String x:Key="m_WeaponType">Aseen tyyppi:</v:String>
    <v:String x:Key="m_Skin">Iho:</v:String>
    <v:String x:Key="m_Quality">Laatu:</v:String>
    <v:String x:Key="m_Outcomes">Tulokset:</v:String>
    <v:String x:Key="m_SearchMode">Hakutila:</v:String>
    <v:String x:Key="m_DesiredFloat">Haluttu kelluvuus:</v:String>
