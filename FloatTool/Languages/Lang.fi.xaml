    <v:String x:Key="m_Skip">Ohita:</v:String>
    <v:String x:Key="m_Start">alkaa</v:String>
    <v:String x:Key="m_Stop">Lopettaa</v:String>
    <v:String x:Key="m_FindOne">Löydä yksi</v:String>
    <v:String x:Key="m_Sort">Järjestellä</v:String>
    <v:String x:Key="m_Descending">Laskeva</v:String>
    <v:String x:Key="m_ThreadCount">Lankamäärä:</v:String>
